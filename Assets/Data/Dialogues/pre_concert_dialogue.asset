  m_Script: {fileID: 11500000, guid: c5d4eef2bb57e4d52a3768910d54381a, type: 3}
  m_Name: pre_concert_dialogue
  m_EditorClassIdentifier: Assembly-CSharp::DialogueSequence
  lines:
  - speaker: REX
    text: "Crowd\u2019s restless. They can feel something\u2019s off \u2014 even
      before a note\u2019s played."
    portrait: {fileID: 2933651739717382301, guid: 0e260a8545833d940833ad70475a43c0, type: 3}
    choices: []
  - speaker: LYRA
    text: "The Federation thinks it\u2019s orchestrating obedience.\r\nWait till
      they hear our overture.\r\n"
    portrait: {fileID: 1070808964735896586, guid: e40b9c8ad5a0de84a824b2b5a174529c, type: 3}
    choices: []
  - speaker: DRAVEN
    text: "Been too long since I hit a real stage. Let\u2019s make it count.\r\n"
    portrait: {fileID: 8717023097682995030, guid: a8dfbd30adc1c5047bf79d270a66372c, type: 3}
    choices: []
  - speaker: YOU
    text: "For every world they silenced.\r\nNo more whispers. No more hiding.\r\nWe
      play \u2014 and the galaxy listens.\r\n"
    portrait: {fileID: -1415270251334004555, guid: 44f32b622977fd74cab25f609ba2f8a4, type: 3}
    choices: []
  - speaker: LYRA
    text: "Then let\u2019s hijack the airwaves.\r\nLet them broadcast freedom."
    portrait: {fileID: 1070808964735896586, guid: e40b9c8ad5a0de84a824b2b5a174529c, type: 3}
    choices: []
  - speaker: YOU
    text: "On my mark\u2026\r\nThree\u2026 two\u2026 one\u2014 Play."
    portrait: {fileID: -1415270251334004555, guid: 44f32b622977fd74cab25f609ba2f8a4, type: 3}
    choices: []
  - speaker: 
    text: "[Music bursts \u2014 the anthem fractures into melody and rhythm. Lights
      flicker. The crowd gasps, then begins to cheer.]"
    portrait: {fileID: 0}
    choices: []
